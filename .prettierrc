{
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "none",
    "printWidth": 140,
    "plugins": ["prettier-plugin-svelte"],
    "pluginSearchDirs": ["."],
    "overrides": [{"files": "*.svelte", "options": {"parser": "svelte"}}],
